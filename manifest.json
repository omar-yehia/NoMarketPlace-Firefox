{
  "manifest_version": 2,
 
  "name": "No Marketplace",
  "version": "0.1.8",
  "description": "Remove Marketplace from Facebook Homepage",
  "icons": {
    "16": "icon1.png",
    "48": "icon1.png",
    "128": "icon1.png" },

  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions" : ["tabs","storage"],
  

  "content_scripts": [{
    "matches": ["http://www.facebook.com/*","https://www.facebook.com/*"],
    "js": ["jQuery-3.3.1.min.js","content.js"]
  }]
 
}
